{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "baseUrl": ".",
  "paths": {
    "@cache/*": [
      "cache/*"
    ],
    "@styles/*": [
      "src/styles/*"
    ],
    "@components/*": [
      "src/components/*"
    ],
    "@artifacts/*": [
      "artifacts/*"
    ],
    "@pages/*": [
      "pages/*"
    ],
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "deploy/00_deploy.js"],
  "exclude": ["node_modules"]
}
